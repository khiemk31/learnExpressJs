doctype html
head
    meta(charset='utf-8')
    link(rel='apple-touch-icon' sizes='76x76' href='https://res.cloudinary.com/cde/image/upload/v1658206315/Icon/logo_cdrkpi.png')
    link(rel='icon' type='image/png' href='https://res.cloudinary.com/cde/image/upload/v1658206315/Icon/logo_cdrkpi.png')
    meta(http-equiv='X-UA-Compatible' content='IE=edge,chrome=1')
    title Model Fashion
    meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport')

    link(href='https://fonts.googleapis.com/css?family=Montserrat:400,700,200' rel='stylesheet')
    link(href='https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css' rel='stylesheet')
    // CSS Files
    style 
        include ./assets/css/bootstrap.min.css
    style 
        include ./assets/css/paper-dashboard.css 
    style 
        include ./assets/demo/demo.css
.wrapper
    .sidebar(data-color='white' data-active-color='danger')
        .logo
            a.simple-text.logo-mini(href='/main')
                .logo-image-small
                    img(src="https://res.cloudinary.com/cde/image/upload/v1658206315/Icon/logo_cdrkpi.png")
            a.simple-text.logo-normal(href='/main') Model Fashion
        .sidebar-wrapper
            ul.nav
                li.active
                    a(href='/main')
                        i.nc-icon.nc-chart-pie-36
                        p Dashboard
                li
                    a(href='/user')
                        i.nc-icon.nc-circle-10
                        p User
                li
                    a(href='/category')
                        i.nc-icon.nc-bullet-list-67
                        p Category
                li
                    a(href='/product')
                        i.nc-icon.nc-align-left-2
                        p Product
                li
                    a(href='#')
                        i.nc-icon.nc-money-coins
                        p Voucher
                li
                    a(href='/bill')
                        i.nc-icon.nc-delivery-fast
                        p Bill
                li
                    a(href='/profile')
                        i.nc-icon.nc-single-02
                        p Profile
                li.active-pro
                    a(href='#')
                        i.nc-icon.nc-user-run
                        p Log Out
    .main-panel
        // Navbar
        nav.navbar.navbar-expand-lg.navbar-absolute.fixed-top.navbar-transparent
            .container-fluid
                .navbar-wrapper
                    .navbar-toggle
                        button.navbar-toggler(type='button')
                            span.navbar-toggler-bar.bar1
                            span.navbar-toggler-bar.bar2
                            span.navbar-toggler-bar.bar3
                    a.navbar-brand(href='javascript:;') Thống Kê
                button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navigation' aria-controls='navigation-index' aria-expanded='false' aria-label='Toggle navigation')
                    span.navbar-toggler-bar.navbar-kebab
                    span.navbar-toggler-bar.navbar-kebab
                    span.navbar-toggler-bar.navbar-kebab
                #navigation.collapse.navbar-collapse.justify-content-end
                    ul.navbar-nav
                        li.nav-item.btn-rotate.dropdown
                            a#navbarDropdownMenuLink.nav-link.dropdown-toggle(href='#' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                i.nc-icon.nc-bell-55
                                p
                                    span.d-lg-none.d-md-block Some Actions
                            .dropdown-menu.dropdown-menu-right(aria-labelledby='navbarDropdownMenuLink')
                                a.dropdown-item(href='#') Action
                                a.dropdown-item(href='#') Another action
                                a.dropdown-item(href='#') Something else here
        // End Navbar
        .content
            .row
                .col-lg-3.col-md-6.col-sm-6
                    .card.card-stats
                        .card-body
                            .row
                                .col-5.col-md-4
                                    .icon-big.text-center.icon-warning
                                        i.nc-icon.nc-money-coins.text-warning
                                .col-7.col-md-8
                                    .numbers
                                        p.card-category.text-danger Tổng Doanh Thu
                                        p.card-title.text-warning= tongDoanhThu +" đ"
                        .card-footer
                            hr
                            .stats
                                a(href='/main')
                                    i.fa.fa-refresh   Update Now
                .col-lg-3.col-md-6.col-sm-6
                    .card.card-stats
                        .card-body
                            .row
                                .col-5.col-md-4
                                    .icon-big.text-center.icon-warning
                                        i.nc-icon.nc-basket.text-success
                                .col-7.col-md-8
                                    .numbers
                                        p.card-category.text-danger Đơn Đã Bán
                                        p.card-title.text-success= donDaGiao 
                        .card-footer
                            hr
                            .stats
                                a(href='/main')
                                    i.fa.fa-refresh   Update Now
                .col-lg-3.col-md-6.col-sm-6
                    .card.card-stats
                        .card-body
                            .row
                                .col-5.col-md-4
                                    .icon-big.text-center.icon-warning
                                        i.nc-icon.nc-delivery-fast.text-primary
                                .col-7.col-md-8
                                    .numbers
                                        p.card-category.text-danger Đang Xử Lý
                                        p.card-title.text-primary= donDangXuLy
                        .card-footer
                            hr
                            .stats
                                a(href='/main')
                                    i.fa.fa-refresh   Update Now
                .col-lg-3.col-md-6.col-sm-6
                    .card.card-stats
                        .card-body
                            .row
                                .col-5.col-md-4
                                    .icon-big.text-center.icon-warning
                                        i.nc-icon.nc-box-2.text-danger
                                .col-7.col-md-8
                                    .numbers
                                        p.card-category.text-danger Giao hàng thất bại
                                        p.card-title.text-danger= donThatBai
                        .card-footer
                            hr
                            .stats
                                a(href='/main')
                                    i.fa.fa-refresh   Update Now
            .row
                .col-md-12
                    .card
                        .card-header
                            h5.card-title Thống Kế Doanh Số
                        .card-body
                            canvas#chartHours(width='400' height='100')
                        .card-footer
                            hr
                            .stats
                                i.fa.fa-history
                                |  Updated 3 minutes ago
                            .stats
                                a(href='/main')
                                    i.fa.fa-refresh   Update Now
            .row
                .col-md-4
                    .card
                        .card-header
                            h5.card-title Email Statistics
                            p.card-category Last Campaign Performance
                        .card-body
                            canvas#chartEmail
                        .card-footer
                            .legend
                                i.fa.fa-circle.text-primary
                                |  Opened
                                i.fa.fa-circle.text-warning
                                |  Read
                                i.fa.fa-circle.text-danger
                                |  Deleted
                                i.fa.fa-circle.text-gray
                                |  Unopened
                            hr
                            .stats
                                i.fa.fa-calendar
                                |  Number of emails sent
                .col-md-8
                    .card.card-chart
                        .card-header
                            h5.card-title NASDAQ: AAPL
                            p.card-category Line Chart with Points
                        .card-body
                            canvas#speedChart(width='400' height='100')
                        .card-footer
                            .chart-legend
                                i.fa.fa-circle.text-info
                                |  Tesla Model S
                                i.fa.fa-circle.text-warning
                                |  BMW 5 Series
                            hr
                            .card-stats
                                i.fa.fa-check
                                |  Data information certified
